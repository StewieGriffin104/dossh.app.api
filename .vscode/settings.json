{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": ["javascript"],
  "files.eol": "\n",
  "prisma.pinToPrisma6": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#bfebee",
    "activityBar.background": "#bfebee",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#d768d0",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#bfebee",
    "statusBar.background": "#97dfe3",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#6fd3d8",
    "statusBarItem.remoteBackground": "#97dfe3",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#97dfe3",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#97dfe399",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#97dfe3"
